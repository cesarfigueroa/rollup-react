{
  "name": "rollup-react",
  "version": "1.0.0",
  "description": "Rollup with React",
  "repository": {
    "type": "git",
    "url": "https://github.com/cesarfigueroa/rollup-react"
  },
  "main": "index.js",
  "scripts": {
    "start": "rollup --config --watch",
    "build": "rollup --config",
    "minify": "uglifyjs --compress --mangle -o dist/bundle.min.js -- dist/bundle.js",
    "compress": "gzip --best --to-stdout --keep --verbose dist/bundle.min.js > dist/bundle.min.js.gz",
    "type-check": "flow check"
  },
  "keywords": [
    "rollup"
  ],
  "author": "Cesar Figueroa",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-external-helpers": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "flow-bin": "0.33.0",
    "rollup": "0.36.3",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-commonjs": "5.0.5",
    "rollup-plugin-flow": "1.0.1",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-watch": "2.5.0",
    "uglify-js": "2.7.4"
  },
  "dependencies": {
    "lodash-es": "4.16.4",
    "react": "15.3.2",
    "react-dom": "15.3.2"
  }
}
